
@{
    List<string> kategoriler = new()
    {
      "Bilgisayar",
      "Elektronik",
      "Telefon"
    };
}


<ul class="list-group">
    @foreach (var item in kategoriler)
    {
        <li class="list-group-item">
            <a href="/Home?kategori=@item">
                @item
            </a>
        </li>      
    }
</ul>
